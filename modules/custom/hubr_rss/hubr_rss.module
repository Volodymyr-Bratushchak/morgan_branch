<?php

/**
 * Implements hook_theme() to add the template definition.
 **/

function hubr_rss_theme($existing, $type, $theme, $path) {
  $theme = [];
  $theme['hubr_rss_template'] = [
    'template' => 'hubr-rss-template',
    'variables' => [
      'main_info' => NULL,
      'selected_items' => NULL,
      'image' => NULL,
    ],
  ];
  return $theme;
}

