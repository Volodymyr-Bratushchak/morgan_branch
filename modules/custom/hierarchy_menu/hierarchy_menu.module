<?php

/**
 * Implements hook_theme() to add the template definition.
 **/

function hierarchy_menu_theme($existing, $type, $theme, $path) {
  $theme = array();
  $theme['hierarchy_menu_template'] = array(
    'template' => 'hierarchy-menu-template',
    'variables' => array(
      'parent_element' => NULL,
      'children_bush' => NULL,
    ),
  );
  return $theme;
}




